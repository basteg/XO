

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width"/>
<title>Статистика</title>
</head>
<body>
    <div class="col-md-offset-1">
        <table class="table-bordered">
            <tr>
            <td><p>Маркер</p></td>
            <td><p>Победитель</p></td>
            <td><p>Кол-во ходов<br/>пользователя</p></td>
            <td><p>Дата</p></td>
            </tr>
            @foreach (var item in ViewBag.sg)
            {
            <tr>
                <td style="text-align:center"><p>@item.Marker</p></td>
                <td><p>@item.Result</p></td>
                <td><p>@item.CountUserStep</p></td>
                <td><p>@item.Data</p></td>
            </tr>
            }
        </table>
    </div>
</body>
</html>